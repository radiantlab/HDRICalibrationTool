{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": [
            "-T",
            "-o",
            "1",
            "-W",
            "-j",
            "-q",
            "3",
            "-g",
            "2",
            "0",
            "-t",
            "0",
            "-b",
            "1.1",
            "-Z",
            {
              "validator": "\\S+"
            }
          ],
          "name": "dcraw_emu",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        "**"
      ]
    },
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "core:app:allow-app-show",
    "core:app:allow-app-hide",
    "dialog:default",
    "fs:default",
    "os:default"
  ]
}